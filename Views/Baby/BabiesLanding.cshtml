@model IEnumerable<Baby> 

@{ 
    ViewData["Title"] = "Feed Details";
    Layout = "_BabyLayout";
}


@foreach (var baby in Model)
{
    <p>
    <h1>@baby.Name</h1>
    @{
        var age = DateTime.Now.Subtract(baby.BirthDateTime); 
    }
    <h3>@baby.Nickname is @age.TotalDays days old today!</h3>
    </p>

}
