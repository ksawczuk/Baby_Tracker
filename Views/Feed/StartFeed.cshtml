@model Baby_Tracker.Models.Feed

@{
    ViewData["Title"] = "Feed";
    //Layout = "_FeedLayout";
}

<h4>Feed</h4>
<hr />
<div class="row">
    <div class="col-md-12" align="center">
        <form asp-action="StartFeed">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group form-check">
                <div class="btn-group-toggle" data-toggle="buttons">
                    <label class="btn btn-secondary active">
                        <input type="checkbox" checked autocomplete="off" asp-for="IsDreamFeed"> @Html.DisplayNameFor(model => model.IsDreamFeed)
                    </label>
                </div>
            </div>
            <div class="form-group">
                <input type="submit" value="Start" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="BabyLanding">Back to Baby Page</a>
</div>




@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}



